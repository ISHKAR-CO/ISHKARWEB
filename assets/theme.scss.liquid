  .font-italic{ font-style: italic; }
  .cart-position-rel{
    &:before, .cartLabel{ display:none; }
  }

  @include media-query($medium-up) {
    .cart-position-rel{ position: relative;
      &:before{ content:attr(data-attr);  }
      &:before,
      .cartLabel{ position:absolute; top:100px; left:0; width:100%; font-size: 70%; color: rgb(84,84,84); text-transform: uppercase; display:block; }
    }
  }
}

