  border-radius: 50px;
  width: 100%;
  background-color: transparent;
  border: none;
  border: 1px solid transparent;
  transition: all 150ms ease-in;
    &:hover, &:focus, &:active{
      background-color: rgba(255, 255, 255, 0.71);
      border-color: rgba(135, 135, 135, 0.71);
      &::placeholder{
        color: black;
      }
    }
}

.search-form__submit{
  position: absolute;
  right: 10px;
  top: 50%;
  background-color: transparent;
  border: none;
  border-radius: 40px;
  transform: translateY(-50%);
