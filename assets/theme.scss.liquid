.template-blocks{ @include flex-direction(row);
  &.align-items-start{ @include align-items(start); }
  .content_detail { max-width: 445px; @include display-flexbox(); @include flex-wrap(wrap);@include flex-direction(column);
    .block-title{ @include align-self(flex-start); letter-spacing:3px; text-shadow: 0px 0px 5px #fff; text-transform: uppercase; padding-right:36px; }
    .content_wrapper,p{ color: #0a0a0a; letter-spacing: 0.5px; font-size:14px; }
    p{ @include align-self(flex-end); text-shadow: 0px 0px 5px #fff; }
  }
  .content_img{ max-width: 400px; width: 100%; position:relative;
    .inner_div{ position:relative; display: inline-block; margin-bottom: -6px; width:100%; }
    img.lazyload,img.lazyloading{ width:100%; height:100%; }
    .img_lazy{
      & + p{
        & + .additional_text{ bottom: 60px; right:auto; }
      }
    }
    .vector_video { position: absolute; top: 0; left: 0;
      & + .additional_text{ bottom: 60px; right:auto; }
    }
    .additional_text{ position: absolute; text-align: left; text-transform: uppercase; font-size: 14px; line-height: 18px; bottom:23px; left: 20px; color: #f8f8f4;
      *{ margin-bottom:0; color: #f8f8f4; }
    }
    .vector_video,.additional_text{ opacity:0; visibility:hidden; transition:opacity 500ms ease-out; -webkit-transition:opacity 500ms ease-out; }
    &:hover{ .vector_video,.additional_text{ opacity:1; visibility:visible; } }
    &.square,&.circle{
      &:before{ padding-bottom:100%; }
    }
    &.circle{
      img{ border-radius:50%; }
    }
    &.rectangle{
      &:before{ padding-bottom:120%; }
    }
    &.square,&.rectangle,&.circle{
      position:relative;
      &:before{ content:''; display:inline-block; width: 100%; float:left; }
      img{ position:absolute; top:0; left:0; width:100%; height:100%; object-fit:cover; object-position:center; }
    }
  }
  &:nth-child(even){
    @include flex-direction(row-reverse);
    .content_detail{ padding-left:45px; }
    .content_img{
      img{ float:left; }
    }
  }
  &:nth-child(odd){
    .content_img{ text-align:right;
      img{ float:right; }
    }
    .content_detail{ padding-right:45px; }
  }
  &:not(:last-child) {
    padding-bottom: 100px;
  }

  .testimonial-block &{
    &:nth-child(odd){
      .content_detail{ padding-right:0; padding-left:35px; }
    }
    &:nth-child(even){
      .content_detail{ padding-right:35px; padding-left:0; }
    }
    &:not(:last-child) {
      padding-bottom: 70px;
    }

    .content_detail{
      .content_wrapper{ margin: 0 auto; -ms-flex-item-align: flex-start !important;
        -webkit-align-self: flex-start !important; align-self: flex-start !important;
        p{ font-style: italic; font-size: 23px; line-height: 30px;
          &:before, &:after{ content:',,'; display:inline-block; }
          &:before{ @include transform(rotate(180deg)); }
          &:after{ @include transform(rotate(180deg) rotatey(0deg) rotatez(180deg) translateY(-14px)); }
        }
        .block-title{ font-size:20px; letter-spacing:0; margin-bottom: 0 !important; }
      }
    }
  }
