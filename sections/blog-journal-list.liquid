{% include 'blog-event-list' %}

{% schema %}
  {
    "name": "Journal List",
    "settings": [
		{
			"type":"html",
			"id":"title",
			"label":"Title"
		},
		{
			"type":"text",
			"id":"sub_title",
			"label":"Sub Title"
		},
		{
			"type":"blog",
			"id":"blog",
			"label":"Blog"
		},
		{
			"type":"checkbox",
			"id":"enable_type",
			"label":"Enable Event Type"
		},
		{
			"type":"checkbox",
			"id":"enable_date",
			"label":"Enable Date"
		},
		{
			"type":"checkbox",
			"id":"enable_more_btn",
			"label":"Enable Read More Button of List"
		},		
		{
			"type":"header",
			"content":"Load More Button"
		},
		{
			"type":"range",
			"id":"pagination_limit",
			"label":"Pagination Limit",
			"min":3,
			"max":40,
			"default":9,
			"step":1
		},
		{
          "type":"checkbox",
          "id":"show_page_load",
          "label":"Enable Page Load",
          "default":true
        },
		{
			"type":"text",
			"id":"more_btn_label",
			"label":"Load More Button Label",
			"default":"Load More"
		},
		{
			"type":"image_picker",
			"id":"load_more_btn_frame",
			"label":"Load More Button Frame"
		}
	]
  }
{% endschema %}